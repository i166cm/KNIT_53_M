using System;

struct Student
{
    public string Прізвище;
    public int НомерЗаліковки;
    public double ОцінкаІнформатики;
    public double ОцінкаМатематичногоАналізу;
    public double ОцінкаАлгебри;
    public double СередняОцінка;
}

class Program
{
    static void Main()
    {
        Student[] students = new Student[10];

        // Заповнення даних для кожного студента
        students[0] = new Student() { Прізвище = "Вербецький", НомерЗаліковки = 1001, ОцінкаІнформатики = 90, ОцінкаМатематичногоАналізу = 85, ОцінкаАлгебри = 95 };
        students[1] = new Student() { Прізвище = "Войнаровський", НомерЗаліковки = 1002, ОцінкаІнформатики = 75, ОцінкаМатематичногоАналізу = 80, ОцінкаАлгебри = 70 };
        students[2] = new Student() { Прізвище = "Герасимюк", НомерЗаліковки = 1003, ОцінкаІнформатики = 85, ОцінкаМатематичногоАналізу = 90, ОцінкаАлгебри = 80 };
        students[3] = new Student() { Прізвище = "Корнійчук", НомерЗаліковки = 1004, ОцінкаІнформатики = 95, ОцінкаМатематичногоАналізу = 70, ОцінкаАлгебри = 75 };
        students[4] = new Student() { Прізвище = "Петрук", НомерЗаліковки = 1005, ОцінкаІнформатики = 80, ОцінкаМатематичногоАналізу = 85, ОцінкаАлгебри = 90 };
        students[5] = new Student() { Прізвище = "Сидорчук", НомерЗаліковки = 1006, ОцінкаІнформатики = 85, ОцінкаМатематичногоАналізу = 75, ОцінкаАлгебри = 80 };
        students[6] = new Student() { Прізвище = "Ступінь", НомерЗаліковки = 1007, ОцінкаІнформатики = 90, ОцінкаМатематичногоАналізу = 80, ОцінкаАлгебри = 85 };
        students[7] = new Student() { Прізвище = "Хельчик", НомерЗаліковки = 1008, ОцінкаІнформатики = 70, ОцінкаМатематичногоАналізу = 90, ОцінкаАлгебри = 75 };
        students[8] = new Student() { Прізвище = "Чачанко", НомерЗаліковки = 1009, ОцінкаІнформатики = 80, ОцінкаМатематичногоАналізу = 85, ОцінкаАлгебри = 70 };
        students[9] = new Student() { Прізвище = "Яворський", НомерЗаліковки = 1010, ОцінкаІнформатики = 75, ОцінкаМатематичногоАналізу = 90, ОцінкаАлгебри = 80 };

        // Розрахунок середньої оцінки для кожного студента
        for (int i = 0; i < students.Length; i++)
        {
            var student = students[i];
            student.СередняОцінка = (student.ОцінкаІнформатики + student.ОцінкаМатематичногоАналізу + student.ОцінкаАлгебри) / 3;
            students[i] = student;
        }

        // Сортування масиву студентів за середньою оцінкою
        Array.Sort(students, (s1, s2) => s2.СередняОцінка.CompareTo(s1.СередняОцінка));

        // Виведення відсортованого списку студентів
        Console.WriteLine("Список студентів, відсортований за середньою оцінкою:");
        foreach (var student in students)
        {
            Console.WriteLine("Прізвище: {0}", student.Прізвище);
            Console.WriteLine("Номер заліковки: {0}", student.НомерЗаліковки);
            Console.WriteLine("Середня оцінка: {0}", student.СередняОцінка);
            Console.WriteLine("--------------------------------------------");
        }
    }
}
