using System;
using System.Collections.Generic;

class Program
{
    static void Main()
    {
        int[] array = { 4, 2, 3, 4, 2, 3, 5, 5, 6, 1, 2 };

        // Створення словника для зберігання кількості зустрічання кожного значення
        Dictionary<int, int> countDict = new Dictionary<int, int>();

        // Перебір елементів масиву та збільшення лічильника для кожного значення
        foreach (int number in array)
        {
            if (countDict.ContainsKey(number))
                countDict[number]++;
            else
                countDict[number] = 1;
        }

        // Виведення повторюючихся значень на екран
        Console.WriteLine("Повторюючіся значення:");

        foreach (var kvp in countDict)
        {
            if (kvp.Value >= 2)
                Console.WriteLine(kvp.Key);
        }
    }
}
